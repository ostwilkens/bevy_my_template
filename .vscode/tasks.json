{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"command": "run",
			"args": [
				"--features",
				"dev"
			],
			"problemMatcher": [
				"$rustc",
				"$rust-panic"
			],
			"presentation": {
				"echo": true,
				"reveal": "always"
			},
			"label": "run"
		},
		{
			"type": "cargo",
			"command": "run",
			"args": [
				"--target",
				"wasm32-unknown-unknown",
				"--release",
				"--features",
				"dev"
			],
			"problemMatcher": [
				"$rustc"
			],
			"group": "none",
			"label": "wasmrun"
		},
		{
			"type": "cargo",
			"command": "build",
			"args": [
				"--features",
				"dev",
				"dyn",
			],
			"problemMatcher": [
				"$rustc"
			],
			"group": "build",
			"label": "rust: cargo build"
		}
	]
}